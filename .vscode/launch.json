{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Android",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "android"
        },
        {
          "name": "IPhone8",
          "cwd": "${workspaceFolder}",
          "type": "reactnative",
          "request": "launch",
          "platform": "ios",
          "target": "iPhone 8"
        },
        {
          "name": "IPhone11",
          "cwd": "${workspaceFolder}",
          "type": "reactnative",
          "request": "launch",
          "platform": "ios",
          "target": "iPhone 11"
        },
        {
          "name": "IPhone11Pro",
          "cwd": "${workspaceFolder}",
          "type": "reactnative",
          "request": "launch",
          "platform": "ios",
          "target": "iPhone 11 Pro"
        },
        {
          "name": "IPhone11ProMAX",
          "cwd": "${workspaceFolder}",
          "type": "reactnative",
          "request": "launch",
          "platform": "ios",
          "target": "iPhone 11 Pro Max"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "svg-reactify",
          "runtimeArgs": ["-r", "ts-node/register"],
          "env": {
            "TS_NODE_PROJECT": "./node_modules/svg-reactify/tsconfig.json"
          },
          "args": [
            "./node_modules/svg-reactify/lib/index.js",
            "-e",
            "./src/components/svgConfigs",
            "-g",
            "./src/components/generated",
            "./assets/svg"
          ]
        }
    ]
}