    "typedoc": "^0.19.2",
    "typescript": "4.0.3",
    "webpack-cli": "^4.0.0",
    "wml": "^0.0.83"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "enzyme-adapter-react-16"
    },
    "setupFiles": [
      "./jestSetup/jestSetup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "scripts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(SampleComponentLib|react-native-web|react-native-svg-charts|react-native|jest|@react-native-community|@react-navigation|react-native-gesture-handler|react-native-reanimated|react-native-modal-datetime-picker|react-native-iphone-x-helper|react-native-screens|react-native-haptic-feedback|react-native-video|react-native-iap|react-native-confirmation-code-field)/)"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
