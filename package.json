{
  "name": "app-template",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "metroKill": "lsof -ti:8081 | xargs kill",
    "rename": "react-native-rename",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 11 Pro'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "Video2HEVC": "./scripts/video2hevc.sh",
    "build-docs": "typedoc --ignoreCompilerErrors --exclude '*/components/generated/**' --exclude '*/__mocks__/**' --out docs/ src/",
    "docs": "yarn build-docs && serve -o docs",
    "web": "node scripts/start.js",
    "build-web": "node scripts/build.js",
    "test-web": "node scripts/test.js --env=jsdom",
    "eject-web": "node scripts/eject.js",
    "start-web": "clear && webpack-dev-server --config config/webpack.config.js",
    "reset-watchman": "watchman watch-del-all && watchman watch-project .",
    "add-link": "zsh scripts/AddLink.sh",
    "remove-link": "zsh scripts/RemoveLink.sh",
    "develop": "zsh scripts/Develop.sh",
    "stop-developing": "zsh scripts/StopDeveloping.sh",
    "startup": "zsh scripts/Startup.sh",
    "activate-links": "zsh scripts/ActivateLinks.sh",
    "svg-reactify": "node ./node_modules/svg-reactify/lib/run.js -e ./src/components/svgConfigs -g ./src/components/generated ./assets/svg"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn test"
    }
  },
